{
  "manifest_version": 3,
  "name": "Deep Dark",
  "description": "Enabled deep dark mode for web pages and dev tools. Saves energy with OLED screens",
  "version": "1.0",
  "icons": {
    "128": "128.png"
  },
 "devtools_page": "devtools.html",
  "permissions": [
    "notifications",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "css": ["dark.css"],
      "js": ["dark.js"]
    }
  ]
}
